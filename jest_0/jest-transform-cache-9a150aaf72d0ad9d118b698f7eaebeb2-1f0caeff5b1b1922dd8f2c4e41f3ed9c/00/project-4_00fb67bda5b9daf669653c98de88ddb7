"use strict";var getFirstItem = function getFirstItem(collection, cb) {
  // invoke the callback function and pass the first item from the collection in as an argument
  cb(collection[0]);
};

var getLength = function getLength(collection, cb) {
  // Write a function called getLength that passes the length of the array into the callback
  cb(collection.length);
};

var getLastItem = function getLastItem(collection, cb) {
  // Write a function called getLastItem which passes the getLastItem item of the array into the callback
  cb(collection[collection.length - 1]);
};

var sumNums = function sumNums(x, y, cb) {
  // Write a function called sumNums that adds two numbers and passes the result to the callback
  cb(x + y);
};

var multiplyNums = function multiplyNums(x, y, cb) {
  // Write a function called multiplyNums that adds two numbers and passes the result to the callback
  cb(x * y);
};

var contains = function contains(collection, item, cb) {
  // Write a function called contains that checks if an item is present inside of the given array.
  // Pass true to the callback if it is, otherwise pass false
  if (collection.includes(item)) {
    cb(true);
  } else {
    cb(false);
  }
};

var removeDuplicates = function removeDuplicates(collection, cb) {
  // Write a function called removeDuplicates that removes all duplicate values from the given array.
  // Pass the array to the callback function.  Do not mutate the original array.
  var arr = [];
  collection.forEach(function (value) {
    if (!arr.includes(value)) {
      arr.push(value);
    }
  });
  cb(arr);
};

module.exports = {
  getFirstItem: getFirstItem,
  getLength: getLength,
  getLastItem: getLastItem,
  sumNums: sumNums,
  multiplyNums: multiplyNums,
  contains: contains,
  removeDuplicates: removeDuplicates };